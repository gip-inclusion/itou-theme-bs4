<header role="banner" id="header">
  <section class="s-header">
    <div class="s-header__container container">
      <div class="s-header__row row">
        <div class="s-header__col s-header__col--logo-ministere col col-md-auto d-flex align-items-center pr-0">
          <a href="#top" class="s-header__logo-ministere"><img src="images/logo-republique-francaise.svg" alt="Rébublique Française"></a>
        </div>
        <div class="s-header__col s-header__col--logo-service col-auto d-flex align-items-center px-0">
          <a href=""><img src="images/<% if (locals.headerLogo != undefined) { %><%= headerLogo %> <% } %>" alt="<% if (locals.headerLogoAlt != undefined) { %><%= headerLogoAlt %><% } %>"></a>
        </div>
        <div class="s-header__col s-header__col--nav col d-none d-xl-inline-flex d-xl-flex align-items-center justify-content-end">
          <nav role="navigation" id="nav-primary" aria-label="Navigation principale">
            <ul>
              <li>
                <a href="" class="btn btn-link" target="_blank">
                  Besoin d'aide ?
                </a>
              </li>
              <li>
                <button class="btn btn-outline-primary">
                  Se connecter
                </button>
              </li>
              <li class="dropdown">
                <button class="btn btn-primary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-controls="loginUserDropdown" aria-expanded="false">
                  Se connecter | S'inscrire
                </button>
                <div class="dropdown-menu" id="loginUserDropdown">
                  <ul class="list-unstyled">
                    <li>
                      <a class="dropdown-item" href="">Candidat</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Prescripteur</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Employeur solidaire</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Institution partenaire</a>
                    </li>
                  </ul>
                </div>
              </li>
            </ul>
          </nav>
        </div>
        <div class="s-header__col s-header__col--burger col d-flex align-items-center justify-content-end d-xl-none" data-toggle="burger">
          <i class="ri-menu-line" id="open" role="button" tabindex="0" aria-label="Ouverture du menu de navigation principale pour mobile"></i>
          <i class="ri-close-line" id="close" role="button" tabindex="0" aria-label="Fermeture du menu de navigation principale pour mobile"></i>
        </div>
        <div id="burgerNav">
          <nav role="navigation" aria-label="Navigation principale pour mobile">
            <ul>
              <li>
                <a href="" class="btn btn-link mh-auto p-0">
                  Besoin d'aide ?
                </a>
              </li>
              <li>
                <button class="btn btn-outline-primary">
                  Se connecter
                </button>
              </li>
              <li class="dropdown">
                <button class="btn btn-primary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-controls="loginUserDropdownMobile" aria-expanded="false">
                  Se connecter | S'inscrire
                </button>
                <div class="dropdown-menu" id="loginUserDropdownMobile">
                  <ul class="list-unstyled">
                    <li>
                      <a class="dropdown-item" href="">Candidat</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Prescripteur</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Employeur solidaire</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Institution partenaire</a>
                    </li>
                  </ul>
                </div>
              </li>
            </ul>
            <ul>
              <li>
                <a href="" class="is-active">Tableau de bord</a>
              </li>
              <li>
                <a href="">Rechercher des emplois inclusifs</a>
              </li>
              <li>
                <a href="">Rechercher des prescripteurs habilités</a>
              </li>
              <li>
                <a href="" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-controls="documentationDropdownMobile" aria-expanded="false">Documentation</a>
                <div class="dropdown-menu" id="documentationDropdownMobile">
                  <ul class="list-unstyled">
                    <li>
                      <a class="dropdown-item" href="">Doc candidat</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Doc prescripteur</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Doc employeur solidaire</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Doc institution partenaire</a>
                    </li>
                  </ul>
                </div>
              </li>
            </ul>
          </nav>
        </div>
      </div>
    </div>
  </section>
  <section class="s-postheader d-none d-xl-block">
    <div class="s-postheader__container container">
      <div class="s-postheader__row row">
        <div class="s-postheader__col col-12 d-flex align-items-center">
          <nav role="navigation" aria-label="Navigation secondaire">
            <ul>
              <li>
                <a href="" class="is-active">Tableau de bord</a>
              </li>
              <li>
                <a href="">Rechercher des emplois inclusifs</a>
              </li>
              <li>
                <a href="">Rechercher des prescripteurs habilités</a>
              </li>
              <li>
                <a href="" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-controls="documentationDropdown" aria-expanded="false">Documentation</a>
                <div class="dropdown-menu" id="documentationDropdown">
                  <ul class="list-unstyled">
                    <li>
                      <a class="dropdown-item" href="" target="_blank">Doc candidat</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Doc prescripteur</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Doc employeur solidaire</a>
                    </li>
                    <li>
                      <a class="dropdown-item" href="">Doc institution partenaire</a>
                    </li>
                  </ul>
                </div>
              </li>
            </ul>
          </nav>
        </div>
      </div>
    </div>
  </section>
</header>
